context UserContextCopy
sets
	USER
	PASSWORD
	ROLE
constants
	root_user
	root_password
	CLIENT
	STAFF
	ADMIN
	NO_USER
axioms
	@NO_USER: NO_USER ∈ USER
	@ROOT_USER: root_user ∈ USER
	@NO_USER-ROOT_USER: root_user ≠ NO_USER
	@ROOT_PASSWORD: root_password ∈ PASSWORD
	@roles: partition(ROLE, {CLIENT}, {STAFF}, {ADMIN})
end