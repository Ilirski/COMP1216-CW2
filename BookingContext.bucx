context BookingContext 
extends PharmacyContext
sets
	APPOINTMENT
	BOOKING_STATUS
	APPOINTMENT_STATUS
constants
	True
	False
	Available
	Accepted
	Rejected
axioms
	@booking-status: partition(BOOKING_STATUS, {True}, {False})
	@appointment-status: partition(APPOINTMENT_STATUS, {Available}, {Accepted}, {Rejected})
end
