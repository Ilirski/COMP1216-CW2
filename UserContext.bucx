context UserContext
sets
	USER
	PASSWORD
	ROLE
constants
	root_user
	root_password
	no_user
	Client
	Staff
	Admin
axioms
	@no-user-is-user: no_user ∈ USER
	@root-user-is-user: root_user ∈ USER
	@root-not-null: root_user ≠ no_user
	@ROOT_PASSWORD: root_password ∈ PASSWORD
	@roles: partition(ROLE, {Client}, {Staff}, {Admin})
end